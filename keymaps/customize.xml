<keymap version="1.0.0" name="customize" parent="Eclipse">
    <action id="ShowIntentionActions">
        <keyboard-shortcut first-keystroke="ctrl 1"/>
    </action>
    <action id="CallHierarchy">
        <keyboard-shortcut first-keystroke="ctrl shift h"/>
    </action>
    <action id="ClassNameCompletion">
        <keyboard-shortcut first-keystroke="alt slash"/>
    </action>
    <action id="CloseContent">
        <keyboard-shortcut first-keystroke="ctrl w"/>
    </action>
    <action id="CloseAllEditors">
        <keyboard-shortcut first-keystroke="ctrl shift w"/>
    </action>
    <action id="CollapseAllRegions">
        <keyboard-shortcut first-keystroke="alt shift c"/>
    </action>
    <action id="ExpandAllRegions">
        <keyboard-shortcut first-keystroke="alt shift x"/>
    </action>
    <action id="EditorStartNewLineBefore">
        <keyboard-shortcut first-keystroke="ctrl shift enter"/>
    </action>
    <action id="EditorToggleCase">
        <keyboard-shortcut first-keystroke="ctrl shift y"/>
    </action>
    <action id="Generate">
        <keyboard-shortcut first-keystroke="shift alt s"/>
    </action>
    <action id="GotoNextError">
        <keyboard-shortcut first-keystroke="ctrl shift q"/>
    </action>
    <action id="MethodHierarchy">
        <keyboard-shortcut first-keystroke="ctrl alt h"/>
    </action>
    <action id="ParameterInfo">
        <keyboard-shortcut first-keystroke="ctrl alt slash"/>
    </action>
    <action id="RevealIn">
        <keyboard-shortcut first-keystroke="ctrl shift x"/>
    </action>
    <action id="ShowUmlDiagram">
        <keyboard-shortcut first-keystroke="ctrl shift u"/>
    </action>
    <action id="Vcs.ShowMessageHistory">
        <keyboard-shortcut first-keystroke="ctrl e"/>
    </action>
</keymap>